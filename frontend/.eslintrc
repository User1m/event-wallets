{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "standard-with-typescript"
  ],
  "overrides": [],
  "ignorePatterns": [
    "cypress/",
    "cypress*",
    "jest*"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json",
    "tsconfigRootDir": "."
  },
  "plugins": [
    "react",
    "jest"
  ],
  "rules": {
    "react/prop-types": "off",
    "@typescript-eslint/explicit-module-boundary-types": "off",
    "@typescript-eslint/explicit-function-return-type": "off",
    "react/no-unescaped-entities": 0,
    "@typescript-eslint/strict-boolean-expressions": "off",
    "@typescript-eslint/restrict-template-expressions": "off",
    "@typescript-eslint/prefer-nullish-coalescing": "off",
    "@typescript-eslint/no-misused-promises": "off",
    "@typescript-eslint/no-non-null-assertion": "off",
    "@typescript-eslint/triple-slash-reference": "off",
    "object-shorthand": 0,
    "@typescript-eslint/semi": "off",
    "no-extra-semi": "off",
    "@typescript-eslint/no-unused-vars": 1,
    "@typescript-eslint/no-floating-promises": "off",
    "@typescript-eslint/prefer-ts-expect-error": "off",
    "@typescript-eslint/indent": 1,
    "@typescript-eslint/comma-dangle": 1,
    "no-trailing-spaces": 1,
    "no-multiple-empty-lines": 1,
    "no-multi-spaces": 1,
    "@typescript-eslint/space-infix-ops": 1,
    "operator-linebreak": 1,
    "@typescript-eslint/member-delimiter-style": 1,
    "@typescript-eslint/object-curly-spacing": 1,
    "@typescript-eslint/space-before-function-paren": 1,
    "@typescript-eslint/keyword-spacing": 1,
    "@typescript-eslint/quotes": 1,
    "semi": 1,
    "multiline-ternary": 0,
    "arrow-spacing": 1,
    "@typescript-eslint/type-annotation-spacing": 1
  }
}
