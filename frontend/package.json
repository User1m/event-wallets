    "apollo-client": "^2.6.10",
    "dayjs": "^1.11.7",
    "formik": "^2.2.9",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-big-calendar": "^1.6.3",
    "react-datepicker": "^4.10.0",
    "react-dom": "^18.2.0",
    "react-google-charts": "^4.0.0",
