    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/client-preset": "^2.0.0",
    "@types/graphql": "^14.5.0",
    "@types/redux-mock-store": "^1.0.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "autoprefixer": "^10.4.13",
    "cypress": "^11.0.1",
    "cypress-if": "^1.10.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.0",
    "lint-staged": ">=10",
    "postcss": "^8.4.18",
    "prettier": "^2.8.0",
    "redux-mock-store": "^1.5.4",
    "tailwindcss": "^3.2.3",
    "ts-jest": "^29.0.5"
  },
  "scripts": {
    "start": "craco start",
    "start:prod": "serve -s build",
    "build": "yarn schema:gen:ci || true && craco build",
    "test": "craco test",
    "eject": "craco eject",
    "compile": "graphql-codegen",
    "watch": "graphql-codegen -w",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src/",
    "schema:gen:ci": "[ -z \"${GRAPHQL_URL}\" ] && echo 'ERROR: export GRAPHQL_URL= env not set....'; yarn apollo codegen:generate --endpoint $GRAPHQL_URL --target=typescript --globalTypesFile=src/graphql/__generated__/globalTypes.ts  --tagName=gql",
    "cypress": "npx cypress open",
    "cypress:open": "npx cypress open",
    "prepare": "husky install",
    "clean:modules": "rm -rf node_modules",
    "ci": "yarn install --ignore-engines",
    "ci:fresh": "yarn install --force --ignore-engines --frozen-lockfile"
  },
  "lint-staged": {
    "*.{css,html}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
