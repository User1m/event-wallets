{
  "name": "nest-typescript-gql-prisma-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript Prisma GQL starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn db:setup && nest build",
    "ci": "yarn install --ignore-engines",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:setup": "yarn prisma:migrate:up && yarn db:gen",
    "start": "yarn prisma:migrate:up && yarn prisma:generate && nest start",
    "start:watch": "yarn prisma:migrate:up && yarn prisma:silent-watch && nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "db": "prisma",
    "prisma:create": "yarn prisma migrate dev",
    "prisma:migrate:up": "yarn prisma migrate deploy",
    "prisma:generate": "yarn prisma generate",
    "prisma:watch": "yarn prisma generate --watch",
    "prisma:silent-watch": "yarn prisma generate --watch &",
    "db:studio": "prisma studio",
    "db:gen": "SKIP_PRISMA_VERSION_CHECK=true prisma generate",
    "db:reset": "SKIP_PRISMA_VERSION_CHECK=true prisma migrate reset --force --preview-feature",
    "db:migrate:dev": "SKIP_PRISMA_VERSION_CHECK=true prisma db push",
    "db:migrate": "SKIP_PRISMA_VERSION_CHECK=true prisma migrate dev",
    "db:migrate:ci": "SKIP_PRISMA_VERSION_CHECK=true prisma migrate",
    "db:seed:deploy": "node dist/prisma/seed.js",
    "db:seed": "yarn prisma db seed",
    "db:seed:dev": "DEBUG='prisma*' yarn prisma db seed",
    "db:gen:migrate": "yarn db:gen && yarn db:migrate",
    "db:gen:migrate:ci": "yarn db:gen && yarn db:migrate:ci",
    "prepare": "husky install "
  },
  "dependencies": {
    "@account-abstraction/sdk": "0.4.0",
    "@nestjs/common": "8.2.6",
    "@nestjs/core": "8.2.6",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/graphql": "8.0.2",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "8.2.6",
    "@nestjs/schedule": "^2.2.0",
    "@prisma/client": "~4.9.0",
    "@prisma/sdk": "~4.0.0",
    "@thirdweb-dev/sdk": "^3.9.5",
    "apollo-server-express": "2.25.0",
    "axios": "^1.3.4",
    "bignumber": "^1.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "commander": "^10.0.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "ethers": "5.7.2",
    "graphql": "15.5.0",
    "graphql-fields": "^2.0.3",
    "graphql-subscriptions": "1.2.1",
    "graphql-type-json": "^0.3.2",
    "graphql-upload-ts": "^2.0.5",
    "husky": "^8.0.3",
    "lodash": "^4.17.21",
    "nodemailer": "^4.6.8",
    "openzeppelin-contracts": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.1.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.0",
    "@nestjs/schematics": "8.0.6",
    "@nestjs/testing": "8.2.6",
    "@prisma/generator-helper": "~4.5.0",
    "@types/express": "4.17.12",
    "@types/graphql-fields": "^1.3.3",
    "@types/jest": "^29.4.0",
    "@types/node": "15.6.1",
    "@types/nodemailer": "^4.6.2",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "dataloader": "^2.0.0",
    "eslint": "7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.23.3",
    "graphql-scalars": "^1.9.3",
    "jest": "27.0.1",
    "lint-staged": "^13.1.2",
    "prettier": "2.3.0",
    "prisma": "^4.9.0",
    "supertest": "6.1.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "9.2.2",
    "ts-node": "10.0.0",
    "tsconfig-paths": "3.9.0",
    "typegraphql-prisma": "^0.23.4",
    "typegraphql-prisma-nestjs": "^0.2200.1",
    "typescript": "4.3.5"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  }
}
